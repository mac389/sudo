P::decrease(X,substance_use,_) :-
	diagnosed_with(X,adhd),
	diagnosed_with(X,sud),
	adolescent(X),
	receive(X,cognitive_behavioral_therapy),
	P is 0.5,
	write('Accessed CTN-0028').

