    <!-- http://www.semanticweb.org/mac389/ontologies/2018/11/untitled-ontology-40#drug_user -->

    <owl:Class rdf:about="http://www.semanticweb.org/mac389/ontologies/2018/11/untitled-ontology-40#drug_user">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mac389/ontologies/2018/11/untitled-ontology-40#substance_user"/>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A drug user is a substance user who self-administers a deliberately manufactured substance (as opposed to a pharmacologically active component of a plant or another organism) so as to bring about at least one certain biological effect.</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">drug user</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/mac389/ontologies/2018/11/untitled-ontology-40#illicit_drug_user -->

    <owl:Class rdf:about="http://www.semanticweb.org/mac389/ontologies/2018/11/untitled-ontology-40#illicit_drug_user">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/mac389/ontologies/2018/11/untitled-ontology-40#drug_user"/>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An illicit drug user is a drug user who self-administers drugs that a political body with legal force has prohibited from administration.</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">illicit drug user</rdfs:label>
    </owl:Class>
    


