        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Drug use refers to the behavior realized by an organism to obtain and consume a substance with biological effects.</rdfs:isDefinedBy>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">drug use</rdfs:label>
    </owl:Class>
    


