//CTN-0020

unemployed(person)
negative_outcome_during_drug_abuse_treatment(person)
negative_outcome_after_drug_abuse_treatment(person)

log(0.8) unemployed(x) => negative_outcome_during_drug_abuse_treatment(x)
log(0.2) !unemployed(x) => negative_outcome_during_drug_abuse_treatment(x)

log(0.8) unemployed(x) => negative_outcome_after_drug_abuse_treatment(x)
log(0.2) !unemployed(x) => negative_outcome_after_drug_abuse_treatment(x)

